    "require": {
        "php": "^7.3",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "acelaya/doctrine-enum-type": "^2.3",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "burgov/key-value-form-bundle": "^1.4",
        "endroid/qr-code-bundle": "^3.2",
        "fervo/enum-bundle": "dev-master",
        "frostiede/composer-dependency-list-bundle": "^1.0",
        "jms/serializer-bundle": "^3.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "lightsaml/lightsaml-idp": "^1.0",
        "lightsaml/symfony-bridge": "^1.1",
        "myclabs/php-enum": "^1.7",
        "nelmio/api-doc-bundle": "^3.2",
        "r/u2f-two-factor-bundle": "^0.7.0",
        "ramsey/uuid-doctrine": "^1.6",
        "rollerworks/password-strength-bundle": "^2.0",
        "scheb/two-factor-bundle": "^3.3",
        "schoolit/adauth-bundle": "dev-master",
        "schoolit/common-bundle": "dev-master",
        "schoolit/idp-exchange-models": "dev-master",
        "schoolit/lightsaml-idp-bundle": "dev-master",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/doctrine-bridge": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/error-handler": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/http-client": "^4.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^4.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^5.0",
        "phpunit/phpunit": "^7.3",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "*",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]

    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01CAXK8XMD4BS4KM21DF4V4T3X",
            "allow-contrib": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/frostieDE/EnumBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/schulit/adauth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/schulit/adauth-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/schulit/common-bundle"
        }
    ]
}
